<?xml version="1.0" encoding="utf-8"?>
<mobile-config>
  <logs>
    <type id="app" path="/data/applogs/broker-service/logs" duration-in-hours="3" enabled="true"/>
    <type id="app-speed" path="/data/applogs/broker-service/logs" duration-in-hours="3" enabled="true"/>
    <type id="error" path="/data/appdatas/cat/app-error-log" duration-in-hours="360" enabled="true"/>
  </logs>
  <checkpoint local-flush-enabled="true">
    <local-flush path="/data/appdatas/cat/app-data-save"/>
  </checkpoint>
  <configs>
    <config id="auto-prune" value="false"/>
    <config id="broker" value="broker-service"/>
    <config id="group-statistics-enabled" value="true"/>
    <config id="remote-ip-service-enabled" value="true"/>
    <config id="dev-mode" value="false"/>
    <config id="invalidatePattern" value="jpg,http:,https:,_middle,_large,_small"/>
  </configs>

