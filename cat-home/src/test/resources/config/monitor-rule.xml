<monitor-rules>
	<rule id="F501InError">
		<metric-item type="regex" monitorCount="false" monitorSum="false" monitorAvg="true">
			f5-2400-1-dianping-com(s).+(s).*(?=inerrors$)
		</metric-item>
		<config starttime="00:00" endtime="24:00">
			<condition minute="3">
				<sub-condition type="MaxVal">500</sub-condition>
			</condition>
		</config>
	</rule>
	<rule id="ErrorAndDiscard">
		<metric-item type="regex">
			f5-2400-1-dianping-com(s).+(s).*(?=outerrors$)
		</metric-item>
		<metric-item type="regex">
			f5-2400-2-dianping-com(s).+(s).*(?=errors$)
		</metric-item>
		<metric-item type="regex">
			f5.*(s).+(s).*(?=discards$)
		</metric-item>
		<metric-item type="regex">
			switch.*(s).+(s).*(?=errors$)
		</metric-item>
		<metric-item type="regex">
			switch.*(s).+(s).*(?=discards$)
		</metric-item>
		<config starttime="00:00" endtime="24:00">
			<condition minute="3">
				<sub-condition type="MaxVal">30</sub-condition>
			</condition>
		</config>
	</rule>
