		<metric-item type="regex" text="f5-2400-1-dianping-com.*(?=inerrors$)"></metric-item>
		<config starttime="00:00" endtime="24:00">
			<condition minute="3">
				<sub-condition type="MaxVal" text="500"></sub-condition>
			</condition>
		</config>
	</rule>
