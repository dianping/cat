<?xml version="1.0" encoding="utf-8"?>
<monitor-rules>
   <rule id="cat;MVC;OutboundPhase;avg">
      <config starttime="00:00" endtime="24:00">
         <condition minute="1" alertType="warning">
            <sub-condition type="MaxVal" text="3000"/>
         </condition>
      </config>
   </rule>
   <rule id="cat;SQL;report-content.insert;count">
      <config starttime="00:00" endtime="24:00">
         <condition minute="1" alertType="warning">
            <sub-condition type="MaxVal" text="1000"/>
         </condition>
      </config>
   </rule>
</monitor-rules>
