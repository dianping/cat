<?xml version="1.0" encoding="utf-8"?>
<monitor-rules>
   <rule id="F501InError">
      <metric-item type="regex">
         f5-2400-1-dianping-com(s).+(s).*(?=inerrors$)
      </metric-item>
      <config starttime="00:00" endtime="24:00">
         <condition minute="3">
            <subcondition type="MaxVal">500</subcondition>
         </condition>
      </config>
   </rule>
</monitor-rules>
