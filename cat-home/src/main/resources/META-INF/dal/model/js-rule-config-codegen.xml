<?xml version="1.0" encoding="UTF-8"?>
<model>
  <entity name="js-rule-config" root="true">
    <entity-ref name="exception-limit" type="list" names="exception-limits" />
  </entity>
  <entity name="exception-limit">
    <attribute name="id" value-type="String" />
    <attribute name="domain" value-type="String" />
    <attribute name="level" value-type="String" />
    <attribute name="limit" value-type="int" />
    <attribute name="mails" value-type="String" />
  </entity>
</model>

