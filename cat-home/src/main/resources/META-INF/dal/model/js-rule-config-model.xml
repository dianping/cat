<?xml version="1.0" encoding="UTF-8"?>
<model model-package="com.dianping.cat.home.js" enable-merger="false"
	enable-sax-parser="true" enable-base-visitor="false">
	<entity name="js-rule-config" root="true">
		<entity-ref name="exception-limit" type="map" names="exception-limits"
			method-find-or-create="true" />
	</entity>
	<entity name="exception-limit">
		<attribute name="id" value-type="String" key="true" />
		<attribute name="domain" value-type="String" />
		<attribute name="level" value-type="String" />
		<attribute name="limit" value-type="int" primitive="true" />
		<attribute name="mails" value-type="String" />
	</entity>
</model>
	

