<?xml version="1.0" encoding="UTF-8"?>
<model model-package="com.dianping.cat.home.business" enable-merger="false"  enable-sax-parser="true"
	enable-base-visitor="false">
  <entity name="business-tag-config" root="true">
    <entity-ref name="tag" type="map" names="tags" />
  </entity>
  <entity name="tag">
    <attribute name="id" value-type="String" key="true" />
    <entity-ref name="business-item" type="list" names="business-items" />
  </entity>
  <entity name="business-item">
    <attribute name="domain" value-type="String" />
    <attribute name="item-id" value-type="String" />
  </entity>
</model>

