<?xml version="1.0" encoding="UTF-8"?>
<model model-package="com.dianping.cat.home.display.policy" enable-sax-parser="true" >
  <entity name="display-policy" root="true">
    <entity-ref name="group" type="map" names="groups" />
  </entity>
  <entity name="group">
    <attribute name="id" value-type="String" key="true" />
    <attribute name="order" value-type="int" />
    <entity-ref name="metric" type="map" names="metrics" />
  </entity>
  <entity name="metric">
    <attribute name="id" value-type="String" key="true" />
    <attribute name="unit" value-type="String" />
    <attribute name="isDelta" value-type="boolean" />
    <attribute name="order" value-type="int" />
    <attribute name="isMonitor" value-type="boolean" default-value="true" />
  </entity>
</model>

