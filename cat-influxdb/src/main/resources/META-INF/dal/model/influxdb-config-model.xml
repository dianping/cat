<?xml version="1.0" encoding="UTF-8"?>
<model model-package="com.dianping.cat.influxdb.config" enable-sax-parser="true" enable-validator="true">
  <entity name="influxdb-config" root="true">
    <entity-ref name="influxdb" type="map" names="influxdbs" />
  </entity>
  <entity name="influxdb">
    <attribute name="id" value-type="String" key="true" />
    <attribute name="host" value-type="String" />
    <attribute name="port" value-type="int" />
    <attribute name="username" value-type="String" />
    <attribute name="password" value-type="String" />
    <attribute name="database" value-type="String" />
  </entity>
</model>

